             xmlns:vm="clr-namespace:SaverMaui.ViewModels"
             x:Class="SaverMaui.Views.RandomContentPage" 
             Title="Random Content">

    <ContentPage.BindingContext>
        <vm:FeedRandomContentViewModel />
